//Then which of your Lordâ€™s favours will you both deny?

#include<bits/stdc++.h>
using namespace std;
#define in(n) scanf("%d",&n);
#define lin(n) scanf("%lld",&n);
#define ll long long

int main(){

    int t;
    in(t);

    while(t--){
     
     char s[129];
     memset(s,'\0',sizeof(s));
     char ch=getchar();
     scanf("%[^\n]",s);
     stack<char>st;
     int n=strlen(s),i;
     for( i=0;i<n;++i){
      if(s[i]=='(' || s[i]=='[')st.push(s[i]);
      else if(st.empty()==false && st.top()=='[' && s[i]==']')st.pop();
      else if(st.empty()==false && st.top()=='(' && s[i]==')')st.pop();
      else break;
     }
    
    if(st.empty() && i==n || n==0)printf("Yes\n");
    else printf("No\n");
   
     }

  return 0;
}
